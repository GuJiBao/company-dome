<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    {{block 'meta'}}{{/block}}
    {{block 'title'}}{{/block}}
    <link rel="stylesheet" href="/web/assets/style/font-awesome.min.css" />
    <link rel="stylesheet" href="/web/assets/style/common.css" />
    {{block 'head'}}{{/block}}
  </head>
  <body>
    <div class="page-top">
      <div class="wrap clearfix">
        <div class="link">
          <span
            ><i class="fa fa-envelope-o" aria-hidden="true"></i
            >{{contact.email}}</span
          >
          <span><i class="fa fa-phone" aria-hidden="true"></i>{{contact.phone}}</span>
        </div>
        <div class="tool clearfix">
            <!-- <div class="lan1"><label class="i18n" name="lan"></label></div> -->
            <!-- <select class="lan2" id="language">
                <option value="zhCN">中文简体</option>
                <option value="en">English</option>
            </select> -->
            <div class="language"><span><img src="/web/assets/images/EN.png" id="language_en"></span><span><img src="/web/assets/images/CN.png" id="language_zhCn"></span></div>
        </div>
      </div>
    </div>
    {{include './common/header.art'}}
    {{block 'content'}}{{/block}}
    {{include './common/footer.art'}}
    <script src="http://libs.baidu.com/jquery/1.11.3/jquery.min.js"></script>
    <script src="/web/assets/js/plugins/axios.min.js"></script>
    <script src="/web/assets/js/plugins/layer/layer.js"></script>
    <script src="/web/assets/js/plugins/jqueryValidate/jquery.validate.min.js"></script>
    <script src="/web/assets/js/plugins/jqueryValidate/localization/messages_zh.min.js"></script>
    <script src="/web/assets/js/plugins/jqueryValidate/additional-methods.js"></script>
    <script src="/web/assets/js/plugins/i18n/jquery.i18n.properties.min.js"></script>
    <script src="/web/assets/js/utils.js"></script>
    <script src="/web/assets/js/language.js"></script>
    <script src="/web/assets/js/common.js"></script>
    {{block 'foot'}}{{/block}}
  </body>
</html>
